
{{#each products}}
    <div class="col-md-3 col-sm-4">
        <div class="card">
            <img class="card-img-top"
                 src="/products/{{this.img}}"
                 alt="Card image cap"/>
            <div class="card-body">
                <h5 class="card-title"> {{this.name}} </h5>
            </div>
        </div>
    </div>
{{/each}}